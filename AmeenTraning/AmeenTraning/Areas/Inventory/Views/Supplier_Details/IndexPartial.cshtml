@model IEnumerable<AmeenTraning.Models.Supplier_Details>

@{

    var Sr = 0;
}

@foreach (var item in Model)
{
    Sr++;
<tr>
    <td>@Sr</td>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Mobile)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Address)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Company_Details.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Created_By)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Modified_By)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Date_Created)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Date_Modified)
    </td>


    <td>
        <a href="/Inventory/Supplier_Details/Edit?id=@item.Supplier_Id" class="btn btn-info">Edit</a> |
        @*<a href="/Inventory/Supplier_Details/Details?id=@item.Supplier_Id" class="btn btn-danger">Details</a> |*@
        <a href="/Inventory/Supplier_Details/Delete?id=@item.Supplier_Id" class="btn btn-danger">Delete</a>
    </td>
</tr>
}




